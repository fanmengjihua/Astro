{
	"version": 3,
	"routes": [
		{
			"src": "^/(.*)/$",
			"headers": {
				"Location": "/$1"
			},
			"status": 308
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/_image$",
			"dest": "_render"
		},
		{
			"src": "^/404$",
			"dest": "_render"
		},
		{
			"src": "^/about$",
			"dest": "_render"
		},
		{
			"src": "^/admin$",
			"dest": "_render"
		},
		{
			"src": "^/api/login$",
			"dest": "_render"
		},
		{
			"src": "^/api/register$",
			"dest": "_render"
		},
		{
			"src": "^/api/resetPassword$",
			"dest": "_render"
		},
		{
			"src": "^/api/saveAuthor$",
			"dest": "_render"
		},
		{
			"src": "^/api/savePost$",
			"dest": "_render"
		},
		{
			"src": "^/api/uploadImage$",
			"dest": "_render"
		},
		{
			"src": "^/authors/page/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/authors/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/authors$",
			"dest": "_render"
		},
		{
			"src": "^/blog/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/contact$",
			"dest": "_render"
		},
		{
			"src": "^/page/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/popular$",
			"dest": "_render"
		},
		{
			"src": "^/search$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}